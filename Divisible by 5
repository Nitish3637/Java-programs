import java.util.Scanner;
public class divisidleby5 {

	public static void main(String[] args) {
		Scanner Sc =new Scanner(System.in);
		System.out.println(" Enter the number :");
		int num = Sc.nextInt();
		if(num%5==0) {
			System.out.println("Given number is divisible 5");
		} else {
			System.out.println("Given is not divisible by 5");
		}
	}

}
